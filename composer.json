{
    "name": "det/members",
    "description": "Laravel Member System API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Det\\Members\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Det\\Members\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "det"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "laravel/sanctum": "^3.0|^4.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Det\\Members\\MemberServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}