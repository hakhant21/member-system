{
    "name": "det/members",
    "description": "Laravel Member System API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DET\\Members\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DET\\Members\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "det"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "laravel/sanctum": "^3.0|^4.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "orchestra/testbench": "^8.0|^9.0",
        "laravel/pint": "^1.13",
        "nunomaduro/larastan": "^2.0"
    },
    "scripts": {
        "pint": "pint",
        "analyse": "phpstan analyse --memory-limit=2G",
        "test": "pest",
        "check-all": [
            "@pint",
            "@analyse",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DET\\Members\\MemberServiceProvider"
            ],
            "aliases": {
                "MemberSystem": "DET\\Members\\Facades\\MemberSystem"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}